
{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth-api",
  "projects": {
    "auth-api": {
      "type": "application",
      "root": "apps/auth-api",
      "entryFile": "main",
      "sourceRoot": "apps/auth-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-api/tsconfig.app.json"
      }
    },
    "dashboard-api": {
      "type": "application",
      "root": "apps/dashboard-api",
      "entryFile": "main",
      "sourceRoot": "apps/dashboard-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dashboard-api/tsconfig.app.json"
      }
    },
    "product-api": {
      "type": "application",
      "root": "apps/product-api",
      "entryFile": "main",
      "sourceRoot": "apps/product-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-api/tsconfig.app.json"
      }
    }
  }
}